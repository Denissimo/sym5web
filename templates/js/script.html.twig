{% block script %}
    {% if user is not defined %}
        {% set user = [] %}
        {% set user = user|merge({'roles':[]}) %}
    {% endif %}

{#    {{ dump(user.user.roles) }}#}

    {% include('js/common.js') %}
    {% include('js/mobile.js') %}
    {% include('js/api.js') %}
    {% for role in user.user.roles %}
        {% if role == 'ROLE_OPERATOR' %}
            {% include('js/role_operator.js') %}
            {% include('js/real.js') %}
            {% include('js/appendLayer3D.js') %}
        {% endif %}
        {% if role == 'ROLE_OWNER' %}
            {% include('js/role_owner.js') %}
            {% include('js/real.js') %}
            {% include('js/appendLayer2D.js') %}
            {% include('js/selectSymbol.js') %}
        {% endif %}
    {% endfor %}
{% endblock %}